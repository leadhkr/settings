  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Authentication Sidecar",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "runtimeArgs": ["run-script", "dev"],
      "port": 5858,
      "env": {
        "ADS_APPLICATION_PORT": "5000",
        "defaultWebServer": "https://dev.blackrock.com",
        "ADS_WHITELISTED_ROUTES": "/tableau/*,/api/fabric/queries",
        "ADS_SESSION_STORE_SECRET": "euMRvZDiPF",
        "ADS_SIDECAR_SECURE": "false",
        "ADS_TLS_REJECT_UNAUTHORIZED": "false",
        "ADS_FILE_STORE_PATH": "/sessions",
        "ADS_LDAP_BIND_DN": "uid=searchuser,ou=people",
        "ADS_LDAP_BIND_CRED": "searchme",
        "ADS_LDAP_SEARCH_FILTER": "(uid={{username}})",
        "ADS_SESSION_NAME": "devdatacatalog",
        "ADS_COOKIE_DOMAIN": ".devkubewd.dev.blackrock.com",
        "ADS_DOMAIN": "devkubewd.dev.blackrock.com",
        "ADS_ENCRYPT_SECRET": "DRN38yiCj4"
      }
    }
  ]
}
